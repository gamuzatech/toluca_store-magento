<config>
    <modules>
        <Gamuza_Brazil>
            <version>0.0.1</version>
        </Gamuza_Brazil>
    </modules>
    <global>
        <helpers>
            <brazil>
                <class>Gamuza_Brazil_Helper</class>
            </brazil>
        </helpers>
        <blocks>
            <brazil>
                <class>Gamuza_Brazil_Block</class>
            </brazil>
        </blocks>
        <models>
            <brazil>
                <class>Gamuza_Brazil_Model</class>
                <resourceModel>brazil_mysql4</resourceModel>
            </brazil>
            <brazil_mysql4>
                <class>Gamuza_Brazil_Model_Mysql4</class>
                <entities />
            </brazil_mysql4>
        </models>
        <resources>
            <brazil_setup>
                <setup>
                    <module>Gamuza_Brazil</module>
                    <class>Mage_Eav_Model_Entity_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </brazil_setup>
            <brazil_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </brazil_write>
            <brazil_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </brazil_read>
        </resources>
    </global>
    <adminhtml>
        <translate>
            <modules>
                <Gamuza_Brazil>
                    <files>
                        <default>Gamuza_Brazil.csv</default>
                    </files>
                </Gamuza_Brazil>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Gamuza_Brazil>
                    <files>
                        <default>Gamuza_Brazil.csv</default>
                    </files>
                </Gamuza_Brazil>
            </modules>
        </translate>
    </frontend>
</config>

